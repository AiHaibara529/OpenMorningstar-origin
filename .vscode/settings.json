{
	"files.exclude": {
		"**/__pycache__": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/VENV": true,
		"**/node_modules": false,
		"**/.idea/": true,
		"whoosh_index": true
	},
	"search.exclude": {
		"TEMP/": true,
		"**/*.csv": true
	},
	/****Todo Tree****/
	"todo-tree.filtering.excludeGlobs": [
		"**/VENV/**",
		"**/node_modules",
	],
	"todo-tree.general.tags": [
		"TODO",
		"FIXME",
		"BUG"
	],
	/****live Sass Compile****/
	"liveSassCompile.settings.formats": [
		{
			"extensionName": ".css",
			// "format": "expanded",
			"format": "compressed"
		}
	],
	"liveSassCompile.settings.generateMap": false,
	/***VSCode Counter***/
	"VSCodeCounter.useGitignore": true,
	"VSCodeCounter.outputPreviewType": "markdown",
	"VSCodeCounter.history": 2,
	/****Github Copilot****/
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": true
	},
	/****Code Spell Checker****/
	"cSpell.ignorePaths": [
		"./pipdeptree.txt",
	],
	"cSpell.words": [
		"Alipay",
		"appkey",
		"appleboy",
		"arithmatex",
		"autoremove",
		"beautifulsoup",
		"betterem",
		"buymeacoffee",
		"colorama",
		"csrfmiddlewaretoken",
		"daisyui",
		"djangorestframework",
		"dotenv",
		"endcompress",
		"fontawesome",
		"fontawesomefree",
		"forloop",
		"GCLOUD",
		"gunicorn",
		"hljs",
		"igraph",
		"ipykernel",
		"ipython",
		"jazzmin",
		"jieba",
